<Bindings>
  <Binding name="COMMODITY_BUYER_AND_SELLER_CONFIRM_BUTTON" runOnUp="true" category="ADDONS" header="COMMODITY_BUYER_AND_SELLER">
    if keystate == 'down' then
      CommodityBuyerAndSeller.confirm()
    end
  </Binding>
  <Binding name="COMMODITY_BUYER_AND_SELLER_STOP" runOnUp="true" category="ADDONS">
    if keystate == 'down' then
      CommodityBuyerAndSeller.stop()
    end
  </Binding>
</Bindings>
